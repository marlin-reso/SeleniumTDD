<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Open Cart Login Suite" verbose="3" thread-count="5"
	parallel="tests">


	<listeners>
		<listener class-name="com.opencart.listeners.ExtentReportListner"></listener>
		<listener class-name="com.opencart.listeners.AllureReportListner"></listener>
	</listeners>

	<test name="Login page Test chrome">
		<parameter name="browser" value="chrome"></parameter>
		 <parameter name="browserversion" value="123.0"/>
		<classes>
			<class name="com.opencart.tests.LoginPageTest" />
		</classes>
	</test>


</suite> <!--
Suite -->
 